<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <display-name>template1</display-name>
            <url-pattern>/index.jsp</url-pattern>
            <url-pattern>/WEB-INF/editMyInfo.jsp</url-pattern>
            <url-pattern>/WEB-INF/gain.jsp</url-pattern>
            <url-pattern>/WEB-INF/editMyLogin.jsp</url-pattern>
            <url-pattern>/WEB-INF/editUserLogin.jsp</url-pattern>
            <url-pattern>/WEB-INF/editUserInfo.jsp</url-pattern>
            <url-pattern>/WEB-INF/showSignUp.jsp</url-pattern>
            <url-pattern>/WEB-INF/editModel.jsp</url-pattern>
            <url-pattern>/WEB-INF/buyModel.jsp</url-pattern>
            <url-pattern>/WEB-INF/addModel.jsp</url-pattern>
            <url-pattern>/WEB-INF/deleteModel.jsp</url-pattern>
            <url-pattern>/WEB-INF/deleteUser.jsp</url-pattern>
            <url-pattern>/WEB-INF/listModels.jsp</url-pattern>
            <url-pattern>/WEB-INF/adminPanel.jsp</url-pattern>
            <page-encoding>UTF-8</page-encoding>
            <include-prelude>/WEB-INF/jspf/header.jspf</include-prelude>
            <include-coda>/WEB-INF/jspf/footer.jspf</include-coda>
        </jsp-property-group>
    </jsp-config>
</web-app>
